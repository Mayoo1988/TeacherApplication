@*@model Education.WebApp.Areas.Admin.Models.TestDetails.TestDetailsModel*@
@model Education.Entity.Admin.TestDetails



<h2>Read Excel File and Display in mvc5</h2>
@using (Html.BeginForm("Index", "TestDetail", FormMethod.Post, new { enctype = "multipart/form-data" }))
{

    @Html.AntiForgeryToken();
    //   @Html.ValidationSummary();
    <table>

        <tr>
            <td>
                @Html.TextBoxFor(M => M.TITLE)
            </td>
        </tr>
        <tr>
            <td>
                @Html.TextBoxFor(M => M.SUBJECTID)

            </td>
        </tr>

        <tr>
            <td>

                @Html.TextBoxFor(M => M.PUBLISHDATE)
            </td>
        </tr>

        <tr>
            <td>
                @Html.TextBoxFor(M => M.GIVENBY)
            </td>
        </tr>


    </table>
    <label class="text-info">Upload Excel File</label>
    <input type="file" class="form-control" name="uploadfile" id="uploadfile" />
    <input type="submit" value="submit" class="btn btn-default" />
}